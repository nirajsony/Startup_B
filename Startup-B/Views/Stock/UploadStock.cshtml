@model Startup_B.Services.stock.Stock

@{
    ViewData["Title"] = "UploadStock";
}

<h1>UploadStock</h1>

<h4>Stock</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="UploadStock" method="post" enctype="multipart/form-data">
            <input type="file" name="file">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="submit" value="Upload Stock Excel" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
